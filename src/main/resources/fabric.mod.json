{
  "schemaVersion": 1,
  "id": "noteblockplayer",
  "version": "${version}",

  "name": "Note Block Player",
  "description": "A client mod to automatically play MIDI files on note blocks.",
  "authors": [
    "commandblockguy"
  ],
  "contact": {
    "sources": "https://github.com/commandblockguy/noteblock-player"
  },

  "icon": "assets/noteblockplayer/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "xyz.commandblockguy.noteblockplayer.NoteBlockPlayer"
    ],
    "cotton-client-commands": ["xyz.commandblockguy.noteblockplayer.Commands"]
  },
  "mixins": [
    "noteblockplayer.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x",
    "libgui": ">=2.3.0",
    "cotton-client-commands": "^1.0.0"
  }
}
